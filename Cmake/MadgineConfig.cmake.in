# - Config file for the FooBar package
# It defines the following variables
#  MADGINE_INCLUDE_DIRS - include directories for FooBar
#  MADGINE_LIBRARIES    - libraries to link against
#  MADGINE_EXECUTABLE   - the bar executable
 
# Compute paths
get_filename_component(MADGINE_CMAKE_DIR "${CMAKE_CURRENT_LIST_FILE}" PATH)
set(MADGINE_INCLUDE_DIRS "@CONF_INCLUDE_DIRS@")
 
# Our library dependencies (contains definitions for IMPORTED targets)
if(NOT TARGET Madgine AND NOT Madgine_BINARY_DIR)
  include("${MADGINE_CMAKE_DIR}/MadgineTargets.cmake")
endif()
 
# These are IMPORTED targets created by MadgineTargets.cmake
set(MADGINE_LIBRARIES OgreMadgine MadgineServer)
set(MADGINE_EXECUTABLE )